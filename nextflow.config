import java.time.*
Date now = new Date()

params {

    timestamp = now.format("yyyyMMdd-HH-mm-ss")
}

profiles {

    standard { includeConfig 'conf/quest.config' }
    quest { includeConfig 'conf/quest.config' }

}


timeline {
    enabled = true
    file = "${params.timestamp}_timeline.html"
}

report {
    enabled = true
    file = "${params.timestamp}_report.html"
}
